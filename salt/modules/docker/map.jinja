{%- import_yaml 'salt/modules/docker/defaults.yaml' as defaults %}
{%- import_yaml 'salt/modules/docker/profilemap.yaml' as profilemap %}


{%- set docker = salt['grains.filter_by'](defaults, grain='kernel', base='default',
  merge=salt['grains.filter_by'](profilemap, grain='vscode-anywhere:profile',
    merge=salt['pillar.get']('vscode-anywhere:docker', {})
  )
) %}