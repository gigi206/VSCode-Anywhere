{%- import_yaml 'salt/modules/ruby/defaults.yaml' as defaults %}
{%- import_yaml 'salt/modules/ruby/profilemap.yaml' as profilemap %}


{%- set ruby = salt['grains.filter_by'](defaults, grain='kernel', base='default',
  merge=salt['grains.filter_by'](profilemap, grain='vscode-anywhere:profile',
    merge=salt['pillar.get']('vscode-anywhere:ruby', {})
  )
) %}