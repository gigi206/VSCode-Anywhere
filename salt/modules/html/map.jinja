{%- import_yaml 'salt/modules/html/defaults.yaml' as defaults %}
{%- import_yaml 'salt/modules/html/profilemap.yaml' as profilemap %}


{%- set html = salt['grains.filter_by'](defaults, grain='kernel', base='default',
  merge=salt['grains.filter_by'](profilemap, grain='vscode-anywhere:profile',
    merge=salt['pillar.get']('vscode-anywhere:html', {})
  )
) %}