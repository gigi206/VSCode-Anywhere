{%- import_yaml 'salt/modules/ansible/defaults.yaml' as defaults %}
{%- import_yaml 'salt/modules/ansible/profilemap.yaml' as profilemap %}


{%- set ansible = salt['grains.filter_by'](defaults, grain='kernel', base='default',
  merge=salt['grains.filter_by'](profilemap, grain='vscode-anywhere:profile',
    merge=salt['pillar.get']('vscode-anywhere:ansible', {})
  )
) %}